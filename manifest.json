{
  "name": "media-sync-manager",
  "version": "1.1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js.js"]
    }
  ],
  "permissions": [
    "tabs",
    "bookmarks"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "title"
  }
}
